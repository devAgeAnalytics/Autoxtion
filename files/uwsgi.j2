[uwsgi]

chdir = {{ install_root }}/{{ project_name }}

env=DJANGO_SETTINGS_MODULE={{ project_name }}.settings.production_settings

module = {{project_name}}.wsgi:application

virtualenv= {{ install_root }}

pythonpath = {{ install_root }}/{{ project_name }}

master = true

processes = 10

socket  = {{ ip }}:{{ uwsgi_port }}

chmod-socket = 664

vacuum = true

enable-threda = true
